{
    "name": "tree-sitter-elpi",
    "version": "1.0.0",
    "description": "Tree sitter parser for the ELPI (Lambda Prolog) programming language",
    "main": "bindings/node",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dom-verity/tree-sitter-elpi.git"
    },
    "tree-sitter": [
        {
            "scope": "source.elpi",
            "file-types": [
                "elpi",
                "mod",
                "sig"
            ],
            "highlights": "queries/highlights.scm",
            "locals": "queries/locals.scm"
        }
    ],
    "keywords": [
        "Parser",
        "ELPI",
        "Tree",
        "Sitter"
    ],
    "author": "Dominic Verity",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dom-verity/tree-sitter-elpi/issues"
    },
    "homepage": "https://github.com/dom-verity/tree-sitter-elpi",
    "dependencies": {
        "nan": "^2.17.0"
    }
}
